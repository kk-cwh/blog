{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "dflydev/apache-mime-types": "^1.0",
        "fideloper/proxy": "~3.3",
        "gregwar/captcha": "^1.1",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "overtrue/laravel-lang": "~3.0",
        "predis/predis": "^1.1",
        "tonyski/netease-cloud-music-for-laravel": "^1.0",
        "tymon/jwt-auth": "1.0.0-rc.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "@php -r \"chmod('vendor/tmd/auto-git-pull/scripts/git-pull.sh', 0755);\"",
            "@php -r \"is_dir('.git/hooks') || mkdir('.git/hooks');\"",
            "@php -r \"file_exists('.git/hooks/pre-commit') || copy('.precommit', '.git/hooks/pre-commit');\"",
            "@php -r \"chmod('.git/hooks/pre-commit', 0755);\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
